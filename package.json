{
  "name": "bot-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node build/deploy-commands.js && node build/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "deploy:commands": "tsc && node ./build/deploy-commands.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.0",
    "@discordjs/voice": "^0.11.0",
    "axios": "^0.27.2",
    "discord.js": "^14.0.3",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.6.1",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
