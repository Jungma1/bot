{
  "name": "bot-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node ./build/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon ./build/index.js\"",
    "dev:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "deploy:commands": "tsc && node ./build/deploy-commands.js"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  }
}
